created(1713927033.5802953).
assert(user_token('715c058f197d6b3dadfeae58641f265d11b4d229',limited_user,[created(1713927033.5739505),comment('An user that has no privileges, used to test authorization errors'),api([]),structures('projects/test-project/structures'),sources('projects/test-project/sources')])).
