<odoo>
<!-- Customer Reject In: Workflow -->
<record id="customer_reject_in_viewer_group" model="res.groups">
    <field name="name">Viewer</field>
    <field name="category_id" ref="customer_reject_in_workflow_module_category" />
</record>

<record id="customer_reject_in_user_group" model="res.groups">
    <field name="name">User</field>
    <field name="category_id" ref="customer_reject_in_workflow_module_category" />
    <field name="implied_ids" eval="[(4, ref('customer_reject_in_viewer_group'))]" />
</record>

<record id="customer_reject_in_validator_group" model="res.groups">
    <field name="name">Validator</field>
    <field name="category_id" ref="customer_reject_in_workflow_module_category" />
    <field name="implied_ids" eval="[(4, ref('customer_reject_in_user_group'))]" />
    <field
            name="users"
            eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"
        />
</record>

<!-- Customer Reject In: Data Ownership -->
<record id="customer_reject_in_company_group" model="res.groups">
    <field name="name">Company</field>
    <field name="category_id" ref="customer_reject_in_data_ownership_module_category" />
</record>

<record id="customer_reject_in_company_child_group" model="res.groups">
    <field name="name">Company and All Child Companies</field>
    <field name="category_id" ref="customer_reject_in_data_ownership_module_category" />
    <field name="implied_ids" eval="[(4, ref('customer_reject_in_company_group'))]" />
</record>

<record id="customer_reject_in_all_group" model="res.groups">
    <field name="name">All</field>
    <field name="category_id" ref="customer_reject_in_data_ownership_module_category" />
    <field
            name="implied_ids"
            eval="[(4, ref('customer_reject_in_company_child_group'))]"
        />
    <field
            name="users"
            eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"
        />
</record>

