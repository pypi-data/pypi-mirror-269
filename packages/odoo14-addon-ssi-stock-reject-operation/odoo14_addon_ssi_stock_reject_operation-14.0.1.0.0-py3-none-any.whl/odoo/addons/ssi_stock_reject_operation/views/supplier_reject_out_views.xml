<odoo>
<record id="supplier_reject_out_action" model="ir.actions.act_window">
    <field name="name">Supplier Reject Out</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">stock.picking</field>
    <field name="view_mode">tree,form</field>
    <field
            name="domain"
            eval="[('picking_type_category_id','=',ref('picking_category_supplier_reject_out'))]"
        />
    <field
            name="context"
            eval="{'default_picking_type_category_id':ref('picking_category_supplier_reject_out')}"
        />
</record>

<menuitem
        id="supplier_reject_out_menu"
        name="Supplier Reject Out"
        parent="ssi_stock.menu_outgoing_movement"
        groups="supplier_reject_out_viewer_group"
        action="supplier_reject_out_action"
        sequence="119"
    />
</odoo>
