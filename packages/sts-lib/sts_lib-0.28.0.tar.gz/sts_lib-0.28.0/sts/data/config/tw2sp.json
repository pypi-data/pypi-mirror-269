{
  "name": "Traditional Chinese (Taiwan standard) to Simplified Chinese (with phrases)",
  "requires": [
    "../OpenCC/config/_default.json"
  ],
  "dicts": [
    {
      "file": "../dictionary/tw2sp.tlist",
      "mode": "join",
      "src": [
        {
          "mode": "load",
          "src": [
            "../OpenCC/dictionary/TWPhrasesRev.list",
            {
              "file": "../dictionary/TWPhrasesRev.list",
              "mode": "expand",
              "placeholders": [
                "%c"
              ],
              "src": [
                "../dictionary/TWPhrasesRev.txt",
                "../dictionary/TWPhrasesRevExpLang.txt"
              ]
            },
            "../OpenCC/dictionary/TWVariantsRevPhrases.txt",
            "../dictionary/TWVariantsRev.txt",
            "../OpenCC/dictionary/TWVariantsRev.list"
          ]
        },
        {
          "mode": "load",
          "src": [
            "../OpenCC/dictionary/TSPhrases.txt",
            "../OpenCC/dictionary/TSCharacters.txt"
          ]
        }
      ]
    }
  ]
}
