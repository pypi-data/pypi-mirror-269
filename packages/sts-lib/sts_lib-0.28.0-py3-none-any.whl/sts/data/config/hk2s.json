{
  "name": "Traditional Chinese (Hong Kong variant) to Simplified Chinese",
  "requires": [
    "../OpenCC/config/_default.json"
  ],
  "dicts": [
    {
      "file": "../dictionary/hk2s.tlist",
      "mode": "join",
      "src": [
        {
          "mode": "load",
          "src": [
            "../OpenCC/dictionary/HKVariantsRevPhrases.txt",
            "../dictionary/HKVariantsRev.txt",
            "../OpenCC/dictionary/HKVariantsRev.list"
          ]
        },
        {
          "mode": "load",
          "src": [
            "../OpenCC/dictionary/TSPhrases.txt",
            "../OpenCC/dictionary/TSCharacters.txt"
          ]
        }
      ]
    }
  ]
}
