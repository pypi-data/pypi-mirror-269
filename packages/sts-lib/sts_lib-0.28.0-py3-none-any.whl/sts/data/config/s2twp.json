{
  "name": "Simplified Chinese to Traditional Chinese (Taiwan standard, with phrases)",
  "requires": [
    "../OpenCC/config/_default.json"
  ],
  "dicts": [
    {
      "file": "../dictionary/s2twp.tlist",
      "mode": "join",
      "src": [
        {
          "mode": "load",
          "src": [
            {
              "mode": "expand",
              "placeholders": [
                "%n"
              ],
              "src": [
                "../dictionary/STPhrases.txt",
                {
                  "file": "../dictionary/STPhrasesExpNum.list",
                  "mode": "expand",
                  "placeholders": [
                    "%n"
                  ],
                  "src": [
                    "../dictionary/STPhrasesExpNum.txt",
                    "../dictionary/STPhrasesExpNumPost.txt"
                  ]
                }
              ]
            },
            "../OpenCC/dictionary/STPhrases.txt",
            "../OpenCC/dictionary/STCharacters.txt"
          ]
        },
        {
          "mode": "load",
          "src": [
            "../dictionary/TWPhrases.txt",
            "../OpenCC/dictionary/TWPhrases.list",
            "../OpenCC/dictionary/TWVariants.txt"
          ]
        }
      ]
    }
  ]
}
